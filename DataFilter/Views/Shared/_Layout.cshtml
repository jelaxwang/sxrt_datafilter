<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jelaxUtility.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>
                    数据筛选&导入</h1>
            </div>
            <br />
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("B2B筛选&导出", "B2BDataExport", "Export")</li>
                    <li>@Html.ActionLink("B2C筛选&导出", "B2CDataExport", "Export")</li>
                    <li>@Html.ActionLink("B2B数据导入", "B2BDataImport", "Import")</li>
                    <li>@Html.ActionLink("B2C数据导入", "B2CDataImport", "Import")</li>
                </ul>
            </div>
        </div>
        <div id="main" style="height: 450px; overflow: scroll">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
